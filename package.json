{
  "name": "@vrbo/catalyst-render",
  "version": "3.0.0",
  "description": "A Hapi.js plugin to aid in server-side rendering using Handlebars and React.",
  "main": "lib/index.js",
  "author": {
    "name": "Expedia Group",
    "email": "oss@expediagroup.com"
  },
  "contributors": [
    "tlivings",
    "tuckbick",
