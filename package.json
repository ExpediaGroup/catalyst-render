    "catalyst",
    "catalyst-render",
    "hapi",
    "hapi.js",
    "handlebars",
    "react",
    "node.js",
    "homeaway",
    "vrbo",
    "expedia",
    "expediagroup"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "@hapi/hapi": ">=20.0.0"
  },
  "dependencies": {
